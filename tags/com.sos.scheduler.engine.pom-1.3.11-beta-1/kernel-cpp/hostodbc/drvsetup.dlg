		    -1,45,10,140,25,SS_NOPREFIX
    CTEXT           "Please wait while setup installs and configures the Microsoft Open Database components.",
		    -1,10,40,180,30,NOT WS_GROUP
    ICON            IDI_ICON,-1,8,8,18,20
END

AEXITSUCCESS DIALOG DISCARDABLE  27, 34, 201, 75
STYLE DS_MODALFRAME | WS_POPUP | WS_VISIBLE | WS_CAPTION
CAPTION "Microsoft ODBC Setup"
FONT 8, "Helv"
BEGIN
