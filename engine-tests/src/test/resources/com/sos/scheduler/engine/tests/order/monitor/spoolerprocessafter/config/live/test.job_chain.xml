            next_state="SUCCESS" error_state="ERROR"/>
    <job_chain_node
            state="initial-Shell-Exit0-DontStopOnError"
            job="/Shell-Exit0-DontStopOnError"
            next_state="SUCCESS" error_state="ERROR"/>
    <job_chain_node
            state="initial-Shell-Exit0-DontStopOnError-SpoolerProcessAfter"
