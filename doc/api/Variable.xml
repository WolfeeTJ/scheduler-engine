<?xml-stylesheet href="api_all_languages.xsl" type="text/xsl"?>


<api.class 
    name        = "Variable" 
    object_name = "variable"
    base_dir    = "../"
    author      = "$Author$"
    date        = "$Date$"
>

<property name="value" default_property="true">
    <com>
        <com.result name="value">
            <com.type type="VARIANT*"/>
        </com.result>
    </com>
</property>


<property name="name">
    <com>
        <com.result>
            <com.type type="BSTR"/>
        </com.result>
    </com>
</property>


