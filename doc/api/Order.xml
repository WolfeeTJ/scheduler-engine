<?xml version="1.0"?>
<!-- $Id$ -->

<?xml-stylesheet href="api_all_languages.xsl" type="text/xsl"?>


<api.class 
    name        = "Order" 
    object_name = "order"
    base_dir    = "../"
    author      = "$Author$"
    date        = "$Date$"
>


<property name="id">
    <com>
        <com.result>
            <com.type type="VARIANT*"/>
        </com.result>
    </com>
</property>


<property name="title">
    <com>
        <com.result>
            <com.type type="BSTR"/>
        </com.result>
    </com>
    
    <title>
        Der Titel ist ein Klartext, der den Auftrag bezeichnet
    </title>
</property>


<property name="priority">
    <com>
        <com.result>
            <com.type type="int"/>
        </com.result>
    </com>
</property>


<property name="job_chain" access="read">
    <com>
        <com.result>
            <com.type class="Job_chain"/>
        </com.result>
    </com>
</property>


<property name="job_chain_node" access="read">
    <com>
        <com.result>
            <com.type class="Job_chain_node"/>
        </com.result>
    </com>
</property>


<property name="job">
    <com access="write">
        <com.result name="job_or_name">
            <com.type type="VARIANT*"/>
        </com.result>
    </com>

    <!-- 
    <com access="putref">
        <com.result>
            <com.type type="Job"/>
        </com.result>
    </com>
    -->

    <com access="read">
        <com.result>
            <com.type type="Job"/>
        </com.result>
    </com>
</property>


<property name="state">
    <com>
        <com.result>
            <com.type type="VARIANT*"/>
        </com.result>
    </com>
</property>


<property name="state_text">
    <com>
        <com.result>
            <com.type type="BSTR"/>
        </com.result>
    </com>
</property>


<property name="error" access="read">
    <com>
        <com.result>
            <com.type class="Error"/>
        </com.result>
    </com>
    
    <not_implemented programming_language="java"/>
</property>


<property name="payload">
    <com>
        <com.result>
            <com.type type="VARIANT*"/>
        </com.result>
    </com>
    
    <!--
    <com access="putref">
        <com.result>
            <com.type type="IUnknown*"/>
        </com.result>
    </com>
    -->
</property>


<method name="payload_is_type">
    <com>
        <com.parameter name="type_name">
            <com.type type="BSTR"/>
        </com.parameter>
        
        <com.result>
            <com.type type="bool"/>
        </com.result>
    </com>
</method>


<method name="setback">
    <com/>
</method>


<property name="run_time" access="read">
    <com>
        <com.result>
            <com.type class="Run_time"/>
        </com.result>
    </com>
</property>


<method name="remove_from_job_chain">
    <com/>
</method>


<property name="string_next_start_time" access="read">
    <com>
        <com.result>
            <com.type type="BSTR"/>
        </com.result>
    </com>
</property>



</api.class>
