<ini_section 
    file     = "sos.ini"
    name     = "licence"
    title    = ""
    base_dir = ""
    author   = "$Author$"
    date     = "$Date$">

    <ini_entries>
        <ini_entry setting="key1" type="licence_key" title="Lizenzschlüssel">
            <description>
                <p>
                    Wenn Sie mehrere Lizenzschlüssel haben, können Sie weitere unter <code>key2=</code>, <code>key3=</code> usw. eintragen.
                    Achten Sie darauf, dass die Nummerierung lückenlos ist.
                </p>
            </description>
        </ini_entry>


    </ini_entries>
    
    <description>
        <p>
            Der Scheduler verlangt einen Lizenzschlüssel, der hier eingetragen werden kann.
        </p>
    </description>
</ini_section>
