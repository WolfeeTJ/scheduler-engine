<xml_element name     = "add_jobs" 
             parent_page = "../xml_commands.xml"
             category = "Kommando"
             title    = "XML-Element &lt;add_jobs&gt;"
             base_dir = "../"
             author   = "$Author$"
             date     = "$Date$">


    <description>
        <p>
            Die Jobs werden mit <code>temporary="yes"</code> hinzugefügt und gestartet.
            Jeder Job wird gelöscht, sobald er endet.
        </p>
        <p>
            Die Jobnamen müssen eindeutig sein.
        </p>
    </description>

    <xml_child_elements>
        <xml_child_element name="job" multiple="yes"/>
    </xml_child_elements>
    
    <xml_answer element="ok"/>
    
</xml_element>    
