<xml_element name          = "waiting_jobs" 
             title         = "Auf einen freien Prozess wartende Jobs"
             category      = "Antwort"
             sub_directory = "answer/"
             base_dir      = "../../"
             parent_page   = "../../xml_commands.xml"
             author        = "$Author$"
             date          = "$Date$">

    <xml_parent_elements>
        <xml_parent_element name="process_class">
        </xml_parent_element>        
    </xml_parent_elements>
    
    <xml_attributes>
        <xml_attribute name="job" type="name">
        </xml_attribute>
    </xml_attributes>
    
    <xml_child_elements>
        <xml_child_element name="job" multiple="yes">
            <description>
                <p>
                    Nur das Attribut <code>job=</code> wird geliefert, keine Kind-Elemente.
                </p>
            </description>
        </xml_child_element>        
    </xml_child_elements>

    <example>
        <pre>
        </pre>
    </example>
        
</xml_element>
