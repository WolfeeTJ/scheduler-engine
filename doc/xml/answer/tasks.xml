<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet href="../../scheduler.xslt" type="text/xsl"?>

<!--$Id: tasks.xml,v 1.3 2004/11/08 18:07:38 jz Exp $-->

<xml_element name          = "tasks" 
             title         = "Liste der laufenden Tasks"
             category      = "Antwort"
             sub_directory = "answer/"
             base_dir      = "../../"
             parent_page   = "../../xml_commands.xml"
             author        = "$Author: jz $"
             date          = "$Date: 2004/11/08 18:07:38 $">

    <xml_parent_elements>
        <xml_parent_element name="job"/>
    </xml_parent_elements>
    
    <xml_attributes>
        <xml_attribute name="count" type="zahl">
            <description>
                Anzahl der laufenden Tasks.
            </description>
        </xml_attribute>
    </xml_attributes>
    
    <xml_child_elements>
        <xml_child_element name="task" multiple="yes"/>
    </xml_child_elements>

</xml_element>
